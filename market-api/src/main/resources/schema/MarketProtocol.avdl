@namespace("io.horizon.market.transport")
protocol MarketProtocol {

	/**
	  * 订阅行情函数
 	  */
	boolean subscribeMarketData(io.horizon.market.transport.inbound.MarketDataSubscribe subscribe);

	/**
	 * 行情类型
	 */
	@namespace("io.horizon.market.transport.enums")
	enum MarketDataType {
		Depth, Fast, Level10, Level20, Level30
	}

	/**
	 * 行情订阅状态
	 */
	@namespace("io.horizon.market.transport.enums")
	enum SubscribeStatus {
		Succeed, PartSucceed, Failed
	}

	/**
	 * 行情订阅 
	 */
	@namespace("io.horizon.market.transport.inbound")
	record MarketDataSubscribe {
		io.horizon.market.transport.enums.MarketDataType type;
		array<string> instrumentCodes;
		string recvAddr;
	}

	/**
	 * 行情订阅回报
	 */
	@namespace("io.horizon.market.transport.outbound")
	record MarketDataSubscribeReply {
		io.horizon.market.transport.enums.SubscribeStatus status;
		array<string> successInstrumentCodes;
		array<string> failInstrumentCodes;
		string msg;
	}

	/**
	 * 深度行情数据结构
	 */
	@namespace("io.horizon.market.transport.outbound")
	record DepthMarketData {
		long timestamp;
		int instrumentId;
		string instrumentCode;
		double lastPrice;
		int volume;
		long turnover;
		array<double> bidPrices;
		array<int> bidVolumes;
		array<double> askPrices;
		array<int> askVolumes;
		int depth;
	}

	/**
	 * 五档快速行情数据结构
	 */
	@namespace("io.horizon.market.transport.outbound")
	record FastMarketData {
		long timestamp;
		int instrumentId;
		string instrumentCode;
		double lastPrice;
		int volume;
		long turnover;
		double bidPrices1;
		double bidPrices2;
		double bidPrices3;
		double bidPrices4;
		double bidPrices5;
		int bidVolumes1;
		int bidVolumes2;
		int bidVolumes3;
		int bidVolumes4;
		int bidVolumes5;
		double askPrices1;
		double askPrices2;
		double askPrices3;
		double askPrices4;
		double askPrices5;
		int askVolumes1;
		int askVolumes2;
		int askVolumes3;
		int askVolumes4;
		int askVolumes5;
	}

